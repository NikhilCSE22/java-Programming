import java.util.Scanner;
public class BuildArrayWithPermutation {

public static void takeInput(Scanner s) {
	System.out.println("\n enter the size of an array");
	int k=s.nextInt();
	int num[]=new int[k];
	int arr[]=new int [k];
	for(int i=0;i<k;i++)
	{
		num[i]=s.nextInt();
	}
	for(int j=0;j<k;j++)
	{
		 arr[j]=num[num[j]];
	System.out.print(arr[j]+" ");
	}
}
public static void main(String[] ars)
{
	Scanner s=new Scanner(System.in);
	System.out.print("enter how many array do you want ");
	int n=s.nextInt();
for(int i=0;i<n;i++)
{
	takeInput(s);
}
}
}
